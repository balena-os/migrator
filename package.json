{
  "name": "migrator",
  "version": "0.2.3",
  "description": "Migrate computer to balenaOS",
  "private": true,
  "homepage": "https://github.com/balena-os/migrator",
  "main": "dist/index.js",
  "repository": "github:balena-os/migrator",
  "bin": {
    "migrator": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "*.md",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.3.2",
    "etcher-sdk": "^8.5.x",
    "inquirer": "^8.2.5"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.8",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.11.18",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "pkg": "5.8.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "migrator",
    "dirname": "migrator",
    "default": ".",
    "commands": "./dist",
    "plugins": [],
    "topicSeparator": " ",
    "topics": {}
  },
  "pkg": {
    "scripts": "dist/*.js",
    "assets": [
      "node_modules/drivelist/build/Release/drivelist.node",
      "node_modules/lzma-native/prebuilds/**/*",
      "node_modules/usb/prebuilds/**/*"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "pkg": "pkg -o dist/migrator -t node18-win .",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "echo \"No tests yet\" && exit 0",
    "version": "oclif readme && git add README.md"
  },
  "author": "Balena Inc. (https://balena.io/)",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  },
  "bugs": "https://github.com/balena-os/migrator/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "versionist": {
    "publishedAt": "2023-05-08T17:44:37.798Z"
  }
}
